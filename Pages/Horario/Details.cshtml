@page
@model ProyectoBaseDD.Pages.Horario.DetailsModel

@{
    ViewData["Title"] = "Detalles";
}

<h1>Detalles</h1>

<div>
    <h4>Horario</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Grupo
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Horario.Grupo)
        </dd>
        <dt class="col-sm-2">
            Horario
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Horario.Horario1)
        </dd>
        <dt class="col-sm-2">
            Aula
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Horario.Aula)
        </dd>
        <dt class="col-sm-2">
           Clave Asignatura
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Horario.CveAsig)
        </dd>
        <dt class="col-sm-2">
            ID Profesor
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Horario.IdProfesor)
        </dd>
    </dl>
</div>
<div>
    <a asp-page="./Edit" asp-route-id="@Model.Horario?.CveAsig">Editar</a> |
    <a asp-page="./Index">Regresar</a>
</div>
